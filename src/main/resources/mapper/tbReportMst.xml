<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boot.demo.aop.application.rms.common.repository.TbReportMstRepository">
    <insert id="insertTbReportMst" parameterType="TbUserRole">
        INSERT INTO TB_REPORT_MST (TITLE, CONTENT, AUTHOR, CERATE_DATETIME)
        VALUES (#{title}, #{content}, #{author}, SYSDATE)
    </insert>
</mapper>
